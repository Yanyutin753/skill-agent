{
  "$schema": "https://modelcontextprotocol.io/schema/mcp.json",
  "mcpServers": {
    "example-weather": {
      "command": "python",
      "args": ["-m", "weather_server"],
      "env": {
        "API_KEY": "your-api-key-here"
      },
      "disabled": true
    },
    "example-database": {
      "command": "uv",
      "args": ["run", "database_server.py"],
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "5432"
      },
      "disabled": true
    },
    "example-files": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/path/to/allowed/directory"],
      "disabled": true
    }
  }
}
